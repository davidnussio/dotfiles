_rpk_topic()
{
    last_command="rpk_topic"

    command_aliases=()

    commands=()
    commands+=("add-partitions")
    commands+=("alter-config")
    commands+=("consume")
    commands+=("create")
    commands+=("delete")
    commands+=("describe")
    if [[ -z "${BASH_VERSION:-}" || "${BASH_VERSINFO[0]:-}" -gt 3 ]]; then
        command_aliases+=("info")
        aliashash["info"]="describe"
    fi
    commands+=("list")
    if [[ -z "${BASH_VERSION:-}" || "${BASH_VERSINFO[0]:-}" -gt 3 ]]; then
        command_aliases+=("ls")
        aliashash["ls"]="list"
    fi
    commands+=("produce")

    flags=()
    two_word_flags=()
    local_nonpersistent_flags=()
    flags_with_completion=()
    flags_completion=()

    flags+=("--brokers=")
    two_word_flags+=("--brokers")
    flags+=("--config=")
    two_word_flags+=("--config")
    flags+=("--help")
    flags+=("-h")
    local_nonpersistent_flags+=("--help")
    local_nonpersistent_flags+=("-h")
    flags+=("--password=")
    two_word_flags+=("--password")
    flags+=("--sasl-mechanism=")
    two_word_flags+=("--sasl-mechanism")
    flags+=("--tls-cert=")
    two_word_flags+=("--tls-cert")
    flags+=("--tls-enabled")
    flags+=("--tls-key=")
    two_word_flags+=("--tls-key")
    flags+=("--tls-truststore=")
    two_word_flags+=("--tls-truststore")
    flags+=("--user=")
    two_word_flags+=("--user")
    flags+=("--verbose")
    flags+=("-v")

    must_have_one_flag=()
    must_have_one_noun=()
    noun_aliases=()
}

