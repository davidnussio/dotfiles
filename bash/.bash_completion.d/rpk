    flags+=("--admin-api-tls-key=")
    two_word_flags+=("--admin-api-tls-key")
    flags+=("--admin-api-tls-truststore=")
    two_word_flags+=("--admin-api-tls-truststore")
    flags+=("--brokers=")
    two_word_flags+=("--brokers")
    flags+=("--config=")
    two_word_flags+=("--config")
    flags+=("--password=")
    two_word_flags+=("--password")
    flags+=("--sasl-mechanism=")
    two_word_flags+=("--sasl-mechanism")
    flags+=("--tls-cert=")
    two_word_flags+=("--tls-cert")
