_rpk_acl_user_create()
{
    last_command="rpk_acl_user_create"

    command_aliases=()

    commands=()

    flags=()
    two_word_flags=()
    local_nonpersistent_flags=()
    flags_with_completion=()
    flags_completion=()

    flags+=("--help")
    flags+=("-h")
    local_nonpersistent_flags+=("--help")
    local_nonpersistent_flags+=("-h")
    flags+=("--mechanism=")
    two_word_flags+=("--mechanism")
    local_nonpersistent_flags+=("--mechanism")
    local_nonpersistent_flags+=("--mechanism=")
    flags+=("--password=")
    two_word_flags+=("--password")
    local_nonpersistent_flags+=("--password")
    local_nonpersistent_flags+=("--password=")
    flags+=("--admin-api-tls-cert=")
    two_word_flags+=("--admin-api-tls-cert")
    flags+=("--admin-api-tls-enabled")
    flags+=("--admin-api-tls-key=")
    two_word_flags+=("--admin-api-tls-key")
    flags+=("--admin-api-tls-truststore=")
    two_word_flags+=("--admin-api-tls-truststore")
    flags+=("--api-urls=")
    two_word_flags+=("--api-urls")
    flags+=("--brokers=")
    two_word_flags+=("--brokers")
    flags+=("--config=")
    two_word_flags+=("--config")
    flags+=("--sasl-mechanism=")
    two_word_flags+=("--sasl-mechanism")
    flags+=("--tls-cert=")
    two_word_flags+=("--tls-cert")
    flags+=("--tls-enabled")
