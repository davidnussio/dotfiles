_rpk_root_command()
{
    last_command="rpk"

    command_aliases=()

    commands=()
    commands+=("acl")
    commands+=("cloud")
    commands+=("cluster")
    commands+=("container")
    commands+=("debug")
    commands+=("generate")
    commands+=("group")
    if [[ -z "${BASH_VERSION:-}" || "${BASH_VERSINFO[0]:-}" -gt 3 ]]; then
        command_aliases+=("g")
        aliashash["g"]="group"
    fi
    commands+=("help")
    commands+=("iotune")
    commands+=("plugin")
    commands+=("redpanda")
    commands+=("topic")
    commands+=("version")
    commands+=("wasm")

    flags=()
    two_word_flags=()
    local_nonpersistent_flags=()
    flags_with_completion=()
    flags_completion=()

    flags+=("--help")
    flags+=("-h")
    local_nonpersistent_flags+=("--help")
    local_nonpersistent_flags+=("-h")
    flags+=("--verbose")
    flags+=("-v")

    must_have_one_flag=()
    must_have_one_noun=()
    noun_aliases=()
}

__start_rpk()
{
    local cur prev words cword split
    declare -A flaghash 2>/dev/null || :
    declare -A aliashash 2>/dev/null || :
    if declare -F _init_completion >/dev/null 2>&1; then
        _init_completion -s || return
    else
        __rpk_init_completion -n "=" || return
    fi

    local c=0
    local flag_parsing_disabled=
    local flags=()
    local two_word_flags=()
    local local_nonpersistent_flags=()
    local flags_with_completion=()
    local flags_completion=()
    local commands=("rpk")
    local command_aliases=()
    local must_have_one_flag=()
    local must_have_one_noun=()
    local has_completion_function=""
    local last_command=""
    local nouns=()
    local noun_aliases=()

    __rpk_handle_word
}

if [[ $(type -t compopt) = "builtin" ]]; then
    complete -o default -F __start_rpk rpk
else
    complete -o default -o nospace -F __start_rpk rpk
fi

# ex: ts=4 sw=4 et filetype=sh
