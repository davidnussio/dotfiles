    local_nonpersistent_flags+=("--deny-principal")
    local_nonpersistent_flags+=("--deny-principal=")
    flags+=("--dry")
    flags+=("-d")
    local_nonpersistent_flags+=("--dry")
    local_nonpersistent_flags+=("-d")
